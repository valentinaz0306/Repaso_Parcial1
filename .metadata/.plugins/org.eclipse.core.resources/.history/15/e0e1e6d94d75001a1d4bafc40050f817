package model;
import processing.core.PApplet;

public class Perro {

	private int id;
	private int edad;
	private String raza;
	private String nombre;
	private String fecha;
	
	private PApplet app;

	public Perro(int id,int edad,String raza,String nombre, String fecha) {
		this.id=id;	
		this.edad=edad;	
		this.raza=raza;	
		this.nombre=nombre;	
		this.fecha=fecha;	
	
}
	
	
}
